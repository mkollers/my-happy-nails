{
  "name": "my-happy-nails",
  "version": "2.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod --stats-json",
    "start": "node --max_old_space_size=4096 dist/server",
    "test": "ng test --code-coverage --watch false --no-progress",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod --no-progress && ng run my-happy-nails:server:production --bundleDependencies all --no-progress",
    "deploy": "gcloud app deploy app.yaml --project my-happy-nails",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "^8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/platform-server": "~8.1.2",
    "@angular/pwa": "^0.801.2",
    "@angular/router": "^8.1.2",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "compression": "^1.7.4",
    "core-js": "^3.1.4",
    "express": "^4.17.1",
    "google-maps": "^3.3.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "^8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/language-service": "~8.1.2",
    "@angular/service-worker": "^8.1.2",
    "@types/google-maps": "^3.2.1",
    "@types/jasmine": "3.3.15",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "codelyzer": "~5.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-junit-reporter": "^1.2.0",
    "node-sass": "^4.12.0",
    "puppeteer": "^1.19.0",
    "protractor": "~5.4.2",
    "ts-loader": "^6.0.4",
    "ts-node": "8.3.0",
    "tslint": "^5.18.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.12"
  }
}
