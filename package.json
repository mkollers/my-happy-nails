{
  "name": "my-happy-nails",
  "version": "2.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build -c production",
    "start": "node --max_old_space_size=4096 dist/server",
    "test": "ng test --code-coverage --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build:ssr",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build -c production && ng run my-happy-nails:server:production",
    "deploy": "npm run build:ssr && gcloud app deploy app.yaml --project my-happy-nails"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
